<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Testinator.Client">

    <Color x:Key="ClientGray">#514D4C</Color>
    <SolidColorBrush x:Key="ClientGrayBrush" Color="{StaticResource ClientGray}" />

    <Color x:Key="ClientWhite">#E3E3E3</Color>
    <SolidColorBrush x:Key="ClientWhiteBrush" Color="{StaticResource ClientWhite}" />

    <Color x:Key="ClientGreen">#009688</Color>
    <SolidColorBrush x:Key="ClientGreenBrush" Color="{StaticResource ClientGreen}" />

    <Color x:Key="ClientRed">#F24040</Color>
    <SolidColorBrush x:Key="ClientRedBrush" Color="{StaticResource ClientRed}" />

</ResourceDictionary>